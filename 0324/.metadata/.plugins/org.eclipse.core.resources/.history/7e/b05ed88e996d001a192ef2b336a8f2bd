package ex05;

public class Sample {

	public static void main(String[] args) {
		//for1();
		for2();
		

	}

	//10까지 반복 출력
	public static void for1() {
		System.out.println("**예제1***");
		for(int i=0; i<=10; i+=2) { //i=i+2와 같다
			System.out.println(i);
		}
	}	
	
	//100까지 반복 출력
	public static void for2() {
		System.out.println("**예제2***");
		int tot=0;
		for(int i=1; i<=100; i++) {
			System.out.println(i);
		}
		
	}
		
}
