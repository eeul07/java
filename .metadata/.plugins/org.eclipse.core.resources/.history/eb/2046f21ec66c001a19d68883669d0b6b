package ex04;

public class Sample {

	public static void main(String[] args) {
		// 자동형변환(작은것→큰것)
		char charValue='B';
		int intValue = charValue;
		System.out.println(intValue);
		
		double doubleValue=charValue;
		System.out.println(doubleValue);
		
		intValue=100;
		doubleValue=intValue;
		System.out.println(doubleValue);
	}

}