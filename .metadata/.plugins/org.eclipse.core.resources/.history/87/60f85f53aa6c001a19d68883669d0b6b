package ex01;

public class Sample {

	public static void main(String[] args) {
		System.out.print("안녕!!!");
		System.out.println("나는 김예슬..");
		
		//정수형변수
		int num1=110;
		int num2=200;
		int num3=num1 + num2;
		int num4=num1 - num2;
		System.out.println("num=" + num1);
		System.out.println("num=" + num2);
		System.out.println("두수더한결과=" + num3);
		System.out.println("두수뺀결과=" + num4);
	}

}
